<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wupai.taiji.model.mapper.FgFeatureMapper">

    <resultMap id="BaseResultMap" type="com.wupai.taiji.model.entity.FgFeature">
            <id property="featureId" column="feature_id" jdbcType="INTEGER"/>
            <result property="featureName" column="feature_name" jdbcType="VARCHAR"/>
            <result property="featureNum" column="feature_num" jdbcType="INTEGER"/>
            <result property="featureRemarke" column="feature_remarke" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        feature_id,feature_name,feature_num,
        feature_remarke
    </sql>
</mapper>
